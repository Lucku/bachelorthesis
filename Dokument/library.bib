@article{Karras2016,
abstract = {Today, outsourcing query processing tasks to remote cloud servers becomes a viable option; such outsourcing calls for encrypting data stored at the server so as to render it secure against eavesdropping adversaries and/or an honest-but-curious server itself. At the same time, to be efficiently managed, outsourced data should be indexed, and even adaptively so, as a side-effect of query processing. Computationally heavy encryption schemes render such outsourcing unattractive; an alternative, Order-Preserving Encryption Scheme (OPES), intentionally preserves and reveals the order in the data, hence is unattractive from the security viewpoint. In this paper, we propose and analyze a scheme for lightweight and indexable encryption, based on linear-algebra operations. Our scheme provides higher security than OPES and allows for range and point queries to be efficiently evaluated over encrypted numeric data, with decryption performed at the client side. We implement a prototype that performs incremental, query-triggered adaptive indexing over encrypted numeric data based on this scheme, without leaking order information in advance, and without prohibitive overhead, as our extensive experimental study demonstrates.},
author = {Karras, Panagiotis and Nikitin, Artyom and Saad, Muhammad and Bhatt, Rudrika and Antyukhow, Denis and Idreos, Stratos},
doi = {10.1145/2882903.2882932},
file = {:Users/lucku/Documents/Studium/6. Semester/Bachelorarbeit/Referenzen/Karras et al/Proceedings of the 2016 International Conference on Management of Data/Karras et al. - 2016 - Adaptive Indexing over Encrypted Numeric Data.pdf:pdf},
isbn = {9781450335317},
issn = {07308078},
journal = {Proceedings of the 2016 International Conference on Management of Data},
number = {iii},
pages = {171--183},
title = {{Adaptive Indexing over Encrypted Numeric Data}},
url = {http://dl.acm.org/citation.cfm?id=2882932&CFID=642414243&CFTOKEN=77056236},
year = {2016}
}
@article{Bajaj2013,
abstract = {TrustedDB is an outsourced database prototype that allows clients to execute SQL queries with privacy and under regulatory compliance constraints without having to trust the service provider. TrustedDB achieves this by leveraging server-hosted tamper-proof trusted hardware in critical query processing stages. TrustedDB does not limit the query expressiveness of supported queries. And, despite the cost overhead and performance limitations of trusted hardware, the costs per query are orders of magnitude lower than any (existing or) potential future software-only mechanisms. TrustedDB is built and runs on actual hardware, and its performance and costs are},
author = {Bajaj, Sumeet and Sion, Radu},
file = {:Users/lucku/Documents/Studium/6. Semester/Bachelorarbeit/Referenzen/Bajaj, Sion/SIGMOD Conference/Bajaj, Sion - 2011 - TrustedDB A Trusted Hardware based Database with Privacy and Data Confidentiality.pdf:pdf},
isbn = {9781450306614},
journal = {SIGMOD Conference},
keywords = {database,trusted hardware},
pages = {205--216},
title = {{TrustedDB : A Trusted Hardware based Database with Privacy and Data Confidentiality}},
year = {2011}
}
@article{Graefe1991,
abstract = {Data compression is widely used in data management to save storage\nspace and network bandwidth. The authors outline the performance\nimprovements that can be achieved by exploiting data compression in\nquery processing. The novel idea is to leave data in compressed state as\nlong as possible, and to only uncompress data when absolutely necessary.\nThey show that many query processing algorithms can manipulate\ncompressed data just as well as decompressed data, and that processing\ncompressed data can speed query processing by a factor much larger than\nthe compression factor},
author = {Graefe, G. and Shapiro, L.D. D},
doi = {10.1109/SOAC.1991.143840},
file = {:Users/lucku/Documents/Studium/6. Semester/Bachelorarbeit/Referenzen/Graefe, Shapiro/Applied Computing, 1991.,Proceedings of the 1991 Symposium on/Graefe, Shapiro - 1991 - Data compression and database performance.pdf:pdf},
isbn = {0-8186-2136-2},
journal = {Applied Computing, 1991.,[Proceedings of the 1991] Symposium on},
keywords = {Comp},
pages = {22--27},
title = {{Data compression and database performance}},
year = {1991}
}
