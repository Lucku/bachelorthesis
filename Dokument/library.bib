@article{Schuster2015,
abstract = {We present VC3, the first practical framework that allows users to run distributed MapReduce computations in the cloud while keeping their code and data secret, and ensuring the correctness and completeness of their results. VC3 runs on unmodified Hadoop, but crucially keeps Hadoop, the operating system and the hypervisor out of the TCB; thus, confidentiality and integrity are preserved even if these large components are compromised. VC3 relies on SGX processors to isolate memory regions on individual computers, and to deploy new protocols that secure distributed MapReduce computations. VC3 optionally enforces region self-integrity invariants for all MapReduce code running within isolated regions, to prevent attacks due to unsafe memory reads and writes. Experimental results on common benchmarks show that VC3 performs well compared with unprotected Hadoop; VC3's average runtime overhead is negligible for its base security guarantees, 4.5{%} with write integrity and 8{%} with read/write integrity.},
author = {Schuster, Felix and Costa, Manuel and Fournet, C??dric and Gkantsidis, Christos and Peinado, Marcus and Mainar-Ruiz, Gloria and Russinovich, Mark},
doi = {10.1109/SP.2015.10},
file = {:Users/lucku/Documents/Studium/6. Semester/Bachelorarbeit/Referenzen/Schuster et al/Proceedings - IEEE Symposium on Security and Privacy/Schuster et al. - 2015 - VC3 Trustworthy data analytics in the cloud using SGX.pdf:pdf},
isbn = {9781467369497},
issn = {10816011},
journal = {Proceedings - IEEE Symposium on Security and Privacy},
pages = {38--54},
title = {{VC3: Trustworthy data analytics in the cloud using SGX}},
volume = {2015-July},
year = {2015}
}
@article{Akin2015,
abstract = {CryptDB  has  been  proposed  as  a  practical   and  secure\rmiddleware  to  protect  databases  deployed  on  semi-honest\rcloud  servers.  While  CryptDB  provides  sufficient  protection\runder  Threat-1,  here  we  demonstrate  that  when  CryptDB  is\rdeployed  to  secure  the  cloud  hosted  database  of  a  realistic  web\rapplication,  an  attacker  to  database  or  a  Malicious  Database\rAdministrator (mDBA)  can  easily  steal  information,   and  even\rescalate  his  privilege  to  become  the  administrator  of  the\rweb  application.  Our  attacks,  fall  under  a  restricted  form\rof  Threat-2  where  we  only  assume  that  the  attackers  or  the\rmDBA  tampers  with  the  CryptDB  protected  database   and  is\ropens  an  ordinary  user  account  through  the  web  application.\rOur  attacks,  are  carried  out  assuming  perfectly  secure  proxy\rand  application  servers.  Therefore,  the  attacks  work  without\rrecovering  the  master  key  residing  on  the  proxy  server.  At\rthe  root  of  the  attack  lies  the  lack  of  any  integrity  checks\rfor  the  data  in  the  CryptDB  database.  We  propose  a  number\rof  practical  countermeasures  to  mitigate  attacks  targeting  the\rintegrity  of  the  CryptDB  database.  We  also  demonstrate  that\rthe  data  integrity  is  not  sufficient  to  protect  the  databases,\rwhen  query  integrity   and  frequency  attacks  are  considered.},
author = {Akin, Ihsan H. and Sunar, Berk},
doi = {10.1109/BDCloud.2014.75},
file = {:Users/lucku/Documents/Studium/6. Semester/Bachelorarbeit/Referenzen/Akin, Sunar/Proceedings - 4th IEEE International Conference on Big Data and Cloud Computing, BDCloud 2014 with the 7th IEEE International Conference on./Akin, Sunar - 2015 - On the difficulty of securing web applications using CryptDB.pdf:pdf},
isbn = {9781479967193},
journal = {Proceedings - 4th IEEE International Conference on Big Data and Cloud Computing, BDCloud 2014 with the 7th IEEE International Conference on Social Computing and Networking, SocialCom 2014 and the 4th International Conference on Sustainable Computing and C},
keywords = {CryptDB,database integrity,frequency attacks,query integrity},
