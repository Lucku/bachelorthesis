@article{Shmueli2010,
abstract = {This article describes the major challenges and design considerations pertaining to database encryption. The article first presents an attack model and the main relevant challenges of data security, encryption overhead, key management, and integration footprint. Next, the article reviews related academic work on alternative encryption configurations pertaining to encryption locus; indexing encrypted data; and key management. Finally, the article concludes with a benchmark using the following design criteria: encryption configuration, encryption granularity and keys storage.},
author = {Shmueli, Erez and Vaisenberg, Ronen and Elovici, Yuval and Glezer, Chanan and Glezer., Chanan},
doi = {10.1145/1815933.1815940},
file = {:Users/lucku/Documents/Studium/6. Semester/Bachelorarbeit/Referenzen/Shmueli et al/ACM SIGMOD Record/Shmueli et al. - 2010 - Database encryption an overview of contemporary challenges and design considerations.pdf:pdf},
issn = {0163-5808},
journal = {ACM SIGMOD Record},
keywords = {database encryption,privacy,security},
number = {3},
pages = {29--34},
title = {{Database encryption: an overview of contemporary challenges and design considerations}},
url = {http://portal.acm.org/citation.cfm?id=1815933.1815940},
volume = {38},
year = {2010}
}
